#sub_wrap{
	max-width:720px;
	margin:0 auto;
	position:relative;
	height:100%;
}
.subTitle{
	width:430px;
	margin: 155px  auto 150px auto;
	z-index:12;
}
.subTitle img{margin:0 auto;}
.subContainer{
	margin:0 47px;
	overflow:hidden;
	padding-bottom:30px;
}
.subContainer.notice{
	margin:0 37px;
}

/* My영업관리 */
ul.tabs{
	margin: 0px;
	padding: 0px;
	list-style: none;
}
ul.tabs li{
	background: none;
	color: #222;
	display: inline-block;
	padding: 10px 15px;
	cursor: pointer;
}

ul.tabs li.current{
	background: #ededed;
	color: #222;
}

.tab-content{
	display: none;
}

.tab-content.current{
	display: inherit;
}
ul.bmTabs{
	display: table;
	margin:0 auto 50px auto;
}
ul.bmTabs.reward{
	width: 100%;
}
ul.bmTabs.measure.ranking{
	margin:0 auto 30px auto;
}
ul.bmTabs.measure.ranking.two{
	margin:0 auto 50px auto;
}
ul.bmTabs li, ul.bmTabs.measure.ranking li{
	float:left;
	margin-right:12px;
	border-radius:50px;
}
ul.bmTabs.measure li{
	margin-right:12px;
}
ul.bmTabs li:last-child{margin-right:0;}
ul.bmTabs li a, ul.bmTabs.measure li a{
	/*border:1px solid #705dcf;*/
	border-radius:50px;
	line-height:66px;
	color:#705dcf;
	font-size:25px;
	font-weight:300;
	font-family: 'OneShinhanM', sans-serif;
	text-align:center;
	width:236px;
	box-sizing:border-box;
	cursor:pointer;
	display:block;
	background-color:#fff;
}
ul.bmTabs.measure li a{
	width:189px;
	border:1px solid #705dcf;
}
ul.bmTabs.measure.ranking li a{
	width:241px;
	border-radius:50px;
	border:1px solid #705dcf;
}
ul.bmTabs li a:hover, ul.bmTabs li a.current, ul.bmTabs.measure li.current, ul.bmTabs.measure li.hover{
	background:#705dcf;
	color:#fff;
}

/* 랭킹 */
.rankingTitle{
	text-align:center;
	font-size:60px;
	letter-spacing:-5px;
	font-weight:600;
	margin-bottom:77px;
}
.rankingTitle span{
	display:block;
	color:#3789dc;
	font-weight:normal;
	font-size:20px;
}
.rankingBox{
	float:left;
	margin:0 0 90px 22px;
}
.rankingBox dl{
	float:left;
	margin:70px 20px 0 0;
	position:relative;
}
.rankingBox dl:last-child{margin-right:0;}
.rankingBox dl.centerBox{margin-top:15px;}
.rankingBox dt{
	width: 54px;
    height: 54px;
    line-height: 54px;
    border-radius: 54px;
    background: #fff;
    text-align: center;
    color: #96b3ff;
    font-size: 20px;
    font-family: 'OneShinhanB', sans-serif;
    position: absolute;
    top: -27px;
    left: 50%;
    margin-left: -31px;
    border: 4px solid #96b3ff;
    z-index: 1;
}
.rankingBox dl:last-child dt{
	border: 4px solid #c8b4ff;
	color: #c8b4ff;
}
.rankingBox dl.centerBox dt{
	background-color:#fff;
	color:#705dcf;
	border:4px solid #705dcf;
	width:68px;
	height:68px;
	box-sizing:border-box;
	top:-34px;
	line-height:68px;
}
.rankingBox dd{
	width:161px;
	height:97px;
	line-height:35px;
	border-radius:30px;
	box-shadow: 1px 0 18px 10px rgb(0 0 0 / 7%);
	font-family: 'OneShinhanB', sans-serif;
	text-align:center;
	font-weight:bold;
	font-size:24px;
	color:#fff;
	padding-top:60px;
	background-color:#96b3ff;
}
.rankingBox dl:last-child dd{
	background-color:#c8b4ff;
}
.rankingBox dd.crown, .rankingBox dl.centerBox dd.crown {
    width: 21px;
    height: 15px;
    border-radius: 0;
    box-shadow: none;
    padding: 0;
    position: absolute;
    top: -50px;
    left: 50%;
    margin-left: -10px;
	background:none;
}
.rankingBox dl.centerBox dd.crown {
	top: -60px;
}
.rankingBox dl.centerBox dd{
	width:214px;
	height:140px;
	background:#705dcf;
	color:#fff;
	line-height:38px;
	padding-top:72px;
	font-size:33px;
}

.rankingBox dl.centerBox dd span{
	font-size:25px;
	display:block;
	color:#fff;
	font-family: 'OneShinhanB', sans-serif;
	font-weight:200;
}
.rankingBox dl dd span{
	font-size:22px;
	display:block;
	color:#fff;
}
.rankingBox dl dd span.myName{font-size:18px; color:#fff;}
.rankingBox dl dd span.myName em{
	color:#fff;
	font-family: 'OneShinhanM', sans-serif;
	font-weight:bold;
	font-size:18px;
}
.rankingBox dl.centerBox dd span.myName em{
	color:#fff;
	font-family: 'OneShinhanB', sans-serif;
	font-weight:bold;
}
.rankingUnit{
	float:left;
	width:100%;
	margin-bottom:80px;
}
.rankingUnit li{
	float:left;
	font-size:20px;
	font-family: 'OneShinhanM', sans-serif;
	color:#705dcf;
}
.rankingUnit li:last-child{
	float:right;
	margin-right:5px;
}
.rankingList, .rankingList dl{
	display:table;
	margin:0 auto;
}
.rankingList dl{
	margin-bottom:18px;
}
.rankingList dt, .rankingList dd{
	float:left;
	width:130px;
	height:46px;
	line-height:46px;
	border-radius:40px;
	text-align:center;
	color:#fff;
	font-size:20px;
	font-family: 'OneShinhanB', sans-serif;
	background:#bfbfbf;
	box-shadow: 1px 0 18px 10px rgb(0 0 0 / 7%);
}
.rankingList.point dt{
	width:207px;
	font-size:18px;
}
.rankingList dt{margin-right:10px;}
.rankingList dd{
	width:376px;
	background:#fff;
	color:#000;
}

.numbering{
	display: table;
    margin: 30px auto 0 auto;
}
.numbering li{
	float:left;
}
.numbering li a{
	margin-right:20px;
	color:#7d7d7d;
	font-size:15px;
}
.numbering li:first-child a,
.numbering li:nth-child(6) a{
	margin-right:30px;
}
.numbering li a:hover, .numbering li a.current{
	font-weight:bold;
	color:#705dcf;
}

/* 210804 비밀번호 찾기 추가 */
.pwFind_wrap{
	background: url(resources/img/sub/findpwIcon.png) top center no-repeat;
    background-position-y: 50px;
}
.find_result{
	font-size:46px;
	font-family: 'OneShinhanL', sans-serif;
	color:#705dcf;
	padding: 200px 0 140px;
	text-align:center;
	line-height:62px;
}
.find_result em.pw{
	display:block;
}
.find_result em.pw input{
	font-weight:bold;
	display:inline-block;
	font-family: 'OneShinhanB', sans-serif;
	border:none;
	font-size:46px;
	vertical-align: baseline;
	width:100%;
}
.find_result em.pw input::placeholder{
	color:#705dcf;
}
.find_result em.pw input[type="text"]{
	color:#705dcf;
	text-align:center;
}

/* My Reward */
.rewardBox dl{
	width: 100%;
    box-sizing: border-box;
    border-radius: 20px;
	margin-bottom:50px;
}
.rewardBox dt{
	background: none;
    width: 100%;
    font-size: 30px;
    text-align: center;
    height: 80px;
    line-height: 80px;
    background-color: #9486db;
    border-radius: 20px 20px 0 0;
    color: #fff;
    font-family: 'OneShinhanB', sans-serif;
    font-weight: bold;
    letter-spacing: normal;
}
.rewardBox dd{
	line-height: 141px;
    float: none;
    text-align: center;
    font-size: 45px;
    background-color: #f1ecff;
    border-radius: 0 0 20px 20px;
	font-family: 'OneShinhanB', sans-serif;
    font-weight: bold;
    color: #715def;
}
.rewardBox{
	padding-top: 15px;
}
.update_date{
	font-size:25px;
	font-family: 'OneShinhanB', sans-serif;
	text-align:center;
	margin:0 0 30px 0;
	color:#705dcf;
	letter-spacing: normal;
}
.rewardCon{
	width:100%;
	clear:both;
	min-height:300px;
}
.bmTabs.reward li{
	float:left;
	margin-right:15px;
}
.bmTabs.reward li:last-child{margin-right:0;}
.bmTabs.reward li{
	letter-spacing: normal;
	display: inline-block;
    width: 48.8%;
    height: 69px;
    line-height: 69px;
    border: 2px solid #705def;
    border-radius: 500px;
    box-sizing: border-box;
    color: #705def;
    text-align: center;
    font-size: 28px;
    font-family: 'OneShinhanB', sans-serif;
    cursor: pointer;
}
.bmTabs.reward li.current,
.bmTabs.reward li.hover{
	background:#705def;
	color:#fff;
}
.promotion_total{
	float:left;
	border-bottom:1px solid #705def;
	padding:0 0 27px;
	width:100%;
}
.promotion_total li{
	float:left;
	letter-spacing: normal;
	font-size:30px;
	font-family: 'OneShinhanB', sans-serif;
	color:#705def;
}
.promotion_total li:last-child{float:right;}
.promotion_table, .charge_table{
	float:left;
	width:100%;
	margin-top:15px;
	padding-bottom: 10px;
}
.promotion_table th, .promotion_table td,
.charge_table th, .charge_table td{
	height:60px;
	line-height:60px;
}
.promotion_table th, .charge_table th{
	color:#705def;
	font-size:25px;
	font-family: 'OneShinhanB', sans-serif;
}
.promotion_table td, .charge_table td{
	font-size:24px;
	font-family: 'OneShinhanM', sans-serif;
	text-align:center;
}
.charge_table.moreCon{
	border-top:1px dashed #a296e0;
	padding-top:20px;
}
.charge_table .moreBtn{
	width:83px;
	height:83px;
	margin:30px auto;
	cursor:pointer;
}

